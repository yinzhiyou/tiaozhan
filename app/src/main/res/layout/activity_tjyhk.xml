<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#ffffff"
    tools:context=".My.TJYHKActivity">
    <include layout="@layout/layout_tab"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#E1E0E1"

        ></View>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#ffffff"
        >
        <TextView
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:text="姓         名"
            android:textColor="#FF333333"
            android:textSize="16dp"
            android:layout_gravity="center"
            android:layout_marginLeft="12dp"
            android:layout_marginRight="30dp"
            />

        <TextView
            android:id="@+id/bdihk_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""
            android:textColor="#FF333333"
            android:textSize="16dp"
            android:layout_gravity="center"
            />
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#E1E0E1"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="12dp"
        ></View>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#ffffff"
        >
        <TextView
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:text="身份证号"
            android:textColor="#FF333333"
            android:textSize="16dp"
            android:layout_gravity="center"
            android:layout_marginLeft="12dp"
            android:layout_marginRight="30dp"
            />

        <TextView
            android:id="@+id/bdihk_sfz"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""
            android:textColor="#FF333333"
            android:textSize="16dp"
            android:layout_gravity="center"
            />
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#E1E0E1"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="12dp"
        ></View>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#ffffff"
        >
        <TextView
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:text="银行卡号"
            android:textColor="#FF333333"
            android:textSize="16dp"
            android:layout_gravity="center"
            android:layout_marginLeft="12dp"
            android:layout_marginRight="30dp"
            />

        <EditText
            android:id="@+id/bdihk_kahao"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="请输入银行卡号"
            android:textColor="#FF9B9B9B"
            android:textSize="16dp"
            android:background="@null"
            android:layout_gravity="center"
            />
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#E1E0E1"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="12dp"
        ></View>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#ffffff"
        >
        <TextView
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:text="预留手机号"
            android:textColor="#FF333333"
            android:textSize="16dp"
            android:layout_gravity="center"
            android:layout_marginLeft="12dp"
            android:layout_marginRight="30dp"
            />

        <EditText
            android:id="@+id/bdihk_shouji"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="请输入银行预留手机号"
            android:textColor="#FF9B9B9B"
            android:textSize="16dp"
            android:background="@null"
            android:layout_gravity="center"
            />
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#E1E0E1"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="12dp"
        ></View>

    <LinearLayout
        android:id="@+id/bdihk_moren_layout"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#ffffff"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="默认提现银行卡"
            android:textColor="#FF333333"
            android:textSize="16dp"
            android:layout_gravity="center"
            android:layout_marginLeft="12dp"
            android:layout_marginRight="30dp"
            />


        <TextView
            android:id="@+id/bdihk_moren"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="否"
            android:textColor="#FF333333"
            android:textSize="16dp"
            android:layout_weight="1"
            android:layout_gravity="center"
            />

        <ImageView
            android:layout_width="18dp"
            android:layout_height="11dp"
            android:background="@mipmap/xiajiantoutwo"
            android:layout_gravity="center"
            android:layout_marginRight="12dp"
            />
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#E1E0E1"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="12dp"
        ></View>


    <RelativeLayout
        android:id="@+id/my_tixian_tx"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_marginTop="40dp"
        android:layout_marginLeft="24dp"
        android:layout_marginRight="24dp"
        android:background="@drawable/login_rounded_corners">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="提交"
            android:textColor="#ffffff"
            android:textSize="16dp"/>

    </RelativeLayout>
</LinearLayout>